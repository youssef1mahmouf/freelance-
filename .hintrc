{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "disown-opener": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-decoration-skip",
          "text-size-adjust",
          "overflow: clip"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  }
}